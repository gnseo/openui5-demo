<mvc:View
	controllerName="sap.ui.demo.basicTemplate.controller.Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	displayBlock="true"
	xmlns:sapform="sap.ui.layout.form"
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:html="http://www.w3.org/1999/xhtml"
	>
		<Page
				title="{parts: [
					{path: 'csn>Vkorg'}, {path: 'csn>Zoabh'}
				]}"
				showNavButton="true"
				navButtonPress="onNavBack"
				>
			<content>
			  <sapform:Form editable="true" id="csn-form" >
			    <sapform:layout>
			      <sapform:ResponsiveGridLayout />
			    </sapform:layout>
			    <sapform:formContainers>
			      <sapform:FormContainer expandable="true">
			        <sapform:title>
			          <core:Title text="title" />
			        </sapform:title>
			        <sapform:formElements>
			          <sapform:FormElement>
			            <sapform:label>
			              <Label text="{csn>/#CSN/Vkorg/@sap:label}" labelFor="Vkorg" />
			            </sapform:label>
			            <sapform:fields>
			              <Input id="Vkorg" value="{csn>Vkorg}" />
			            </sapform:fields>
			          </sapform:FormElement>
			          <sapform:FormElement>
			            <sapform:label>
			              <Label text="{csn>/#CSN/Zoabh/@sap:label}" labelFor="Zoabh" />
			            </sapform:label>
			            <sapform:fields>
			              <Input id="Zoabh" value="{csn>Zoabh}" />
			            </sapform:fields>
			          </sapform:FormElement>
			        </sapform:formElements>
			      </sapform:FormContainer>
				      <sapform:FormContainer expandable="true">
				        <sapform:title>
				          <core:Title text="title" />
				        </sapform:title>
				        <sapform:formElements>
				          <sapform:FormElement>
				            <sapform:label>
				              <Label text="{csn>/#CSN/Onum/@sap:label}" labelFor="Onum" />
				            </sapform:label>
				            <sapform:fields>
				              <Input id="Onum" value="{csn>Onum}" />
				            </sapform:fields>
				          </sapform:FormElement>
				          <sapform:FormElement>
				            <sapform:label>
				              <Label text="{csn>/#CSN/Zcydwbh/@sap:label}" labelFor="Zcydwbh" />
				            </sapform:label>
				            <sapform:fields>
				              <Input id="Zcydwbh" value="{csn>Zcydwbh}" />
				            </sapform:fields>
				          </sapform:FormElement>
				        </sapform:formElements>
				      </sapform:FormContainer>
			    </sapform:formContainers>
			  </sapform:Form>
      </content>
		</Page>
</mvc:View>
